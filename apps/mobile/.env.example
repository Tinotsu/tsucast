# Supabase
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# API
EXPO_PUBLIC_API_URL=https://api.tsucast.com

# Google OAuth (from Google Cloud Console)
# Required for Google Sign-In - not available in Expo Go
EXPO_PUBLIC_GOOGLE_EXPO_CLIENT_ID=xxxxx.apps.googleusercontent.com
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=xxxxx.apps.googleusercontent.com
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=xxxxx.apps.googleusercontent.com
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=xxxxx.apps.googleusercontent.com

# RevenueCat (for in-app purchases)
EXPO_PUBLIC_REVENUECAT_IOS=appl_xxxxx
EXPO_PUBLIC_REVENUECAT_ANDROID=goog_xxxxx

# PostHog Analytics (optional â€” no-op if unset)
EXPO_PUBLIC_POSTHOG_KEY=
